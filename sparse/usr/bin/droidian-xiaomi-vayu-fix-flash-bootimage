#!/bin/sh

#if [ -z "${LAUNCHED_BY_SYSTEMD}" ]; then
    ## The default getprop binary defined by flash-bootimage script is not working for xiaomi vayu
    ## This script replaces the GETPROP var definition by the getprop bin in /usr/bin

    sed -i 's|^GETPROP="$(choose_application.*|GETPROP="/usr/bin/getprop"|g' /usr/sbin/flash-bootimage

    exit 0
#fi
#
