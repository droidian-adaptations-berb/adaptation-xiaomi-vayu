[Unit]
Description=Place the encryption-supported file if does not exist
Before=phosh.service

[Service]
Type=oneshot
ExecStart=/bin/bash -c 'if [ ! -d "/usr/lib/droidian/device/encryption-supported" ]; then touch /usr/lib/droidian/device/encryption-supported; fi'

[Install]
WantedBy=multi-user.target
